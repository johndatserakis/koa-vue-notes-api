{
  "apps" : [
      {
        "name"        : "koa-vue-notes-api",
        "script"      : "index.js",
        "merge_logs"  : true,
        "env": {
          "NODE_ENV": "production",
          "PORT": 4000
        },
        "instances": 2,
        "exec_mode"  : "cluster_mode",
        "autorestart": true,
        "log_date_format": "YYYY-MM-DD HH:mm Z"
      }
  ]
}